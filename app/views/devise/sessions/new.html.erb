<div class="sty-login z-depth-2">

  <%= image_tag 'icon512x512.png', class:"img" %>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="input-field sty-caja-login">
      <%= f.email_field :email, autofocus: true, class:"validate" %>
      <%= f.label :email %>
    </div>

    <div class=" input-field sty-caja-login">
      <%= f.password_field :password, autocomplete: "off" %>
      <%= f.label :password %>
    </div>
    <div>
      <button type="submit"  class="btn waves-effect waves-light btn">
         <i class="fa fa-sign-in"></i> 
        Entrar
      </button>
    </div>
     <% if devise_mapping.rememberable? -%>
      <div class="login-space-guar">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me,"Guardar sesiÃ³n" %>
      </div>
    <% end -%>
  <% end %>
  <div class="login-space-olv">
  <%= render "devise/shared/links" %>
  </div>
</div>